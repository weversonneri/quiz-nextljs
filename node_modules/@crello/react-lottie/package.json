{
  "name": "@crello/react-lottie",
  "version": "0.0.11",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "private": false,
  "repository": {
    "url": "https://github.com/crello/react-lottie",
    "type": "git"
  },
  "license": "MIT",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/react": "16.8.25",
    "@types/react-dom": "16.8.5",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "concurrently": "4.1.1",
    "eslint": "6.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "husky": "^4.3.0",
    "prepend-file": "1.3.1",
    "prettier": "1.18.2",
    "rollup": "1.19.4",
    "rollup-plugin-typescript2": "0.22.1",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "react": "~16.9.0",
    "react-dom": "~16.9.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm run build:watch\" \"npm run start:example\"",
    "start:example": "cd example && yarn start",
    "github:pages:deploy": "cd example && npm run deploy",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "install:all": "yarn && cd example && yarn",
    "lint:check": "eslint ./src/**/*.ts ./src/**/*.tsx ./example/src/**/*.ts ./example/src/**/*.tsx",
    "lint:fix": "npm run lint:check -- --fix"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "lottie-web": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:check",
      "pre-push": "yarn lint:check"
    }
  },
  "keywords": [
    "react",
    "lottie",
    "lottie-web",
    "react-lottie",
    "animation",
    "bodymovin",
    "canvas",
    "svg",
    "after effects",
    "typescript"
  ]
}
